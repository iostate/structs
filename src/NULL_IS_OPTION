fn main() {
    // rust has no null value
    // we have option enum
    enum Option<T> {
        Some(T),
        None,
    }
    // Must always handle the None case
    let some_numb = Some(5);
    let some_str = Some("a stringy");
    let some_other_type = Some(190);

    let absent_number: Option<i32> = None;
}
